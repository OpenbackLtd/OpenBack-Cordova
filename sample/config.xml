<?xml version='1.0' encoding='utf-8'?>
<widget id="com.openback.cordovasample" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>OpenBackCordovaSample</name>
    <description>
        A sample Apache Cordova application using the OpenBack framework.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-minSdkVersion" value="15" />
        <preference name="com.openback.gcmSenderId" value="G-123456" />
        <resource-file src="openback.json" target="assets/openback.json" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="com.openback.enableRemoteNotifications" value="true" />
        <preference name="com.openback.minimumFetchInterval" value="0" />
    </platform>

    <plugin name="cordova-plugin-openback" spec=".." />
    <!-- plugin name="cordova-plugin-firebase" spec="0.1.12" /-->
    <preference name="com.openback.applicationId" value="OPENBACK_APP_CODE" />
</widget>
